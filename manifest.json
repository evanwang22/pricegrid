{
  "manifest_version": 2,

  "name": "Pricegrid",
  "description": "Expand prices for color and size combinations into a grid",
  "version": "0.1",

  "browser_action": {
    "default_icon": "app/icon.png",
    "default_popup": "app/popup.html",
    "default_title": "Pricegrid"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}